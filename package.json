{
  "name": "minigames",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "mkdir minigames && ln -s ../docs/assets minigames/assets && vite; rm -r minigames",
    "build": "mv docs/assets ./assets.bak && vite build --outDir docs && mv ./assets.bak docs/assets",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^18.16.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "peerjs": "^1.4.7"
  }
}